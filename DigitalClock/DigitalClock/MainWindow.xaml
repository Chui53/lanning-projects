<Window x:Name="ClockFace" x:Class="DigitalClock.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DigitalClock"
        mc:Ignorable="d"
        Title="World Clock" Height="260" Width="500">
    <Grid>
        <TextBlock x:Name="Time" TextAlignment="Center" Margin="10,0,10,0" TextWrapping="Wrap" Text="00:00:00" VerticalAlignment="Top" Height="150" FontSize="112"/>
        <ComboBox x:Name="TimeBox" Margin="62,171,62,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
